{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "programName": {
      "type": "string"
    },
    "instructions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "Categorie": {
            "type": "string"
          },
          "Mnemnonique": {
            "type": "string"
          },
          "Format": {
            "type": "string"
          },
          "Reg/M": {
            "type": "string"
          },
          "Depl": {
            "type": "string"
          },
          "Destinataire": {
            "type": "string"
          },
          "Source": {
            "type": "string"
          }
        },
        "required": [
          "Categorie",
          "Mnemnonique",
          "Format",
          "Reg/M",
          "Depl",
          "Destinataire",
          "Source"
        ]
      }
    }
  },
  "required": [
    "programName",
    "instructions"
  ]
}
